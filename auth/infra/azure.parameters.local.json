{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "bot${{RESOURCE_SUFFIX}}"
    },
    "botAadAppClientId": {
      "value": "${{BOT_ID}}"
    },
    "botAadAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "webAppSKU": {
      "value": "B1"
    },
    "botDisplayName": {
      "value": "Chat with your data"
    },
    "appBackendEndpoint": {
      "value": "${{APP_BACKEND_ENDPOINT}}"
    },
    "botAppDomain": {
      "value": "${{BOT_DOMAIN}}"
    },
    "botAddAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "oauthConnectionName": {
      "value": "${{OAUTH_CONNECTION_NAME}}"
    },
    "appBackendAadAppClientId": {
      "value": "${{APP_BACKEND_AAD_APP_ID}}"
    }
  }
}
